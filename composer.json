{
    "name": "dcplibrary/laravel-microsoft-sso",
    "description": "Laravel package for Microsoft SSO integration with JWT token authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DC Public Library",
            "email": "blashbrook@dcplibrary.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DcpLibrary\\MicrosoftSso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DcpLibrary\\MicrosoftSso\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DcpLibrary\\MicrosoftSso\\Providers\\MicrosoftSsoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
